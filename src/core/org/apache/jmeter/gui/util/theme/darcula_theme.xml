<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE RSyntaxTheme SYSTEM "theme.dtd">
<!--
    Theme that mimics IntelliJ IDEA's defaults.
    See theme.dtd and org.fife.ui.rsyntaxtextarea.Theme for more information.
-->
<RSyntaxTheme version="1.0">

   <!-- Omitting baseFont will use a system-appropriate monospaced. -->
   <!--<baseFont family="..." size="13"/>-->
   
   <!--  General editor colors. -->
   <background color="45494a" />
   <caret color="bbbbbb" />
   <selection fg="b0b4b9" bg="0d293e" />
   <currentLineHighlight color="2c2c2f" fade="true" />
   <marginLine fg="b0b4b9" />
   <markAllHighlight color="ccccff" />
   <markOccurrencesHighlight color="ccccff" border="false" />
   <matchedBracket fg="99ccff" bg="99ccff" highlightBoth="true" animate="false" />
   <hyperlinks fg="0000ff" />
   <secondaryLanguages>
      <language index="1" bg="fff0cc" />
      <language index="2" bg="dafeda" />
      <language index="3" bg="ffe0f0" />
   </secondaryLanguages>

   <!-- Gutter styling. -->
   <gutterBorder color="dddddd" />
   <lineNumbers fg="787878" />
   <foldIndicator fg="808080" iconBg="ffffff" />
   <iconRowHeader activeLineRange="3399ff" />

   <!-- Syntax tokens. -->
   <tokenStyles>
      <style token="IDENTIFIER" fg="ffffff" />
      <style token="RESERVED_WORD" fg="647cca" bold="true" />
      <style token="RESERVED_WORD_2" fg="647cca" bold="true" />
      <style token="ANNOTATION" fg="ef8963" />
      <style token="COMMENT_DOCUMENTATION" fg="e3afed" italic="true" />
      <style token="COMMENT_EOL" fg="e3afed" italic="true" />
      <style token="COMMENT_MULTILINE" fg="e3afed" italic="true" />
      <style token="COMMENT_KEYWORD" fg="e3afed" bold="true" underline="true" italic="true" />
      <style token="COMMENT_MARKUP" fg="e3afed" bg="e2ffe2" italic="true" />
      <style token="DATA_TYPE" fg="647cca" bold="true" />
      <style token="FUNCTION" fg="55d265" />
      <style token="LITERAL_BOOLEAN" fg="98e1ad" bold="true" />
      <style token="LITERAL_NUMBER_DECIMAL_INT" fg="e6e14d" />
      <style token="LITERAL_NUMBER_FLOAT" fg="e6e14d" />
      <style token="LITERAL_NUMBER_HEXADECIMAL" fg="e6e14d" />
      <style token="LITERAL_STRING_DOUBLE_QUOTE" fg="98e1ad" bold="true" />
      <style token="LITERAL_CHAR" fg="e6e14d" bold="true" />
      <style token="LITERAL_BACKQUOTE" fg="e198de" bold="true" />
      <style token="MARKUP_TAG_DELIMITER" fg="e5b3e5" bold="true" />
      <style token="MARKUP_TAG_NAME" fg="e5b3e5" bold="true" />
      <style token="MARKUP_TAG_ATTRIBUTE" fg="e5b3e5" bold="true" />
      <style token="MARKUP_TAG_ATTRIBUTE_VALUE" fg="e5b3e5" bold="true" />
      <style token="MARKUP_COMMENT" fg="e5b3e5" italic="true"/>
      <style token="MARKUP_DTD" fg="e5b3e5"/>
      <style token="MARKUP_PROCESSING_INSTRUCTION"  fg="e5b3e5"/>
      <style token="MARKUP_CDATA" fg="e5b3e5"/>
      <style token="MARKUP_CDATA_DELIMITER" fg="e5b3e5"/>
      <style token="MARKUP_ENTITY_REFERENCE" fg="e5b3e5"/>
      <style token="OPERATOR" fg="ffffff" />
      <style token="PREPROCESSOR" fg="ffffff" />
      <style token="REGEX" fg="ffffff" />
      <style token="SEPARATOR" fg="ffffff" />
      <style token="VARIABLE" fg="ffffff" bold="true" />
      <style token="WHITESPACE" fg="000000" />

      <style token="ERROR_IDENTIFIER" fg="ff0000" />
      <style token="ERROR_NUMBER_FORMAT" fg="ff0000" />
      <style token="ERROR_STRING_DOUBLE" fg="ff0000" />
      <style token="ERROR_CHAR" fg="ff0000" />
   </tokenStyles>

</RSyntaxTheme>